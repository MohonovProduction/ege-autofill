{
  "manifest_version": 3,
  "name": "Ege Autofill",
  "description": "Автоматическая подстановка полей на сайте ege.spb.ru",
  "version": "1.0.0",

  "action": {
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },

  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "https://www.ege.spb.ru/result/"
  ]

}
